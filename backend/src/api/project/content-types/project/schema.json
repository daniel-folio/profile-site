{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "22. Project(프로젝트)",
    "description": "프로젝트 정보"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "displayName": "Title(프로젝트명)"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "displayName": "Slug(슬러그)"
    },
    "shortDescription": {
      "type": "richtext",
      "displayName": "Short Description(요약설명)"
    },
    "fullDescription": {
      "type": "richtext",
      "displayName": "Full Description(상세설명)"
    },
    "thumbnailImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "displayName": "Thumbnail(썸네일)"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "displayName": "Images(이미지)"
    },
    "technologies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill.skill",
      "inversedBy": "projects"
    },
    "projectType": {
      "type": "enumeration",
      "enum": [
        "Web",
        "Mobile",
        "Desktop",
        "API",
        "Library",
        "Other"
      ],
      "required": true,
      "displayName": "Project Type(유형)"
    },
    "projectStatus": {
      "type": "enumeration",
      "enum": [
        "Completed",
        "In Progress",
        "Planned",
        "On Hold"
      ],
      "required": true,
      "displayName": "Project Status(상태)"
    },
    "startDate": {
      "type": "string",
      "displayName": "Start Date(시작년월)"
    },
    "endDate": {
      "type": "string",
      "displayName": "End Date(종료년월)"
    },
    "githubUrl": {
      "type": "string",
      "displayName": "GitHub URL(깃허브)"
    },
    "liveUrl": {
      "type": "string",
      "displayName": "Live URL(배포주소)"
    },
    "featured": {
      "type": "boolean",
      "default": false,
      "displayName": "Featured(대표)"
    },
    "order": {
      "type": "integer",
      "default": 0,
      "displayName": "Order(정렬순서)"
    },
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company",
      "displayName": "Company(회사)",
      "required": false
    },
    "skills": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill.skill",
      "displayName": "Technologies(기술스택)",
      "required": false
    },
    "visible": {
      "type": "boolean",
      "default": true,
      "displayName": "Visible(공개여부)"
    }
  }
}
